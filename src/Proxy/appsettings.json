{
    "ReverseProxy": {
        "Routes": {
            "files": {
                "ClusterId": "files-cluster",
                "Match": {
                    "Path": "/files/{**remainder}"
                }
            },
            "api": {
                "ClusterId": "api-cluster",
                "Match": {
                    "Path": "{**catch-all}"
                }
            }
        },
        "Clusters": {
            "files-cluster": {
                "Destinations": {
                    "files-cluster/destination1": {
                        "Address": "https://files.bitwarden.com"
                    }
                }
            },
            "api-cluster": {
                "Destinations": {
                    "api-cluster/destination1": {
                        "Address": "https://api.bitwarden.com"
                    }
                }
            }
        }
    }
}